within Pharmacolibrary.Drugs.ATC.H;

model H01CC53
  extends Pharmacokinetic.Models.PK_1C_enteral(
    weight         = 70,
    F              = 0.5,
    Cl             = 0.0002,
    adminDuration  = 600,
    adminMass      = 300 / 1000000,   // mg → kg
    adminCount     = 1,
    Vd             = 0.12,
    Cmin           = 0.001,
